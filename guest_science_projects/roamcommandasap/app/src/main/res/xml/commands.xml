<?xml version="1.0" encoding="utf-8"?>
<apkInfo>
    <shortName>roamcommandasap</shortName> <!-- Optional -->
    <primary>true</primary> <!-- Required -->
    <commands>
      <!--
      commands for each individual test. Might add one "modifiable" command.
      name: name of command in GDS e.g., "Command-1"

      Note that syntax is JSON. Keep "name" the same, followed by command string. GDS user can edit
      either of these properties, but that makes it harder to use.
      -->
       <command
            name="StopTest"
            syntax="{&quot;name&quot;: &quot;command-1&quot;}" />
        <command
            name="Test1"
            syntax="{&quot;name&quot;: &quot;command1&quot;}" />
        <command
            name="Test2"
            syntax="{&quot;name&quot;: &quot;command2&quot;}" />
